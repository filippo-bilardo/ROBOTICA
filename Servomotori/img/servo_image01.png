<svg xmlns="http://www.w3.org/2000/svg" width="400" height="300" viewBox="0 0 400 300">
  <!-- Servo Motor Body -->
  <rect x="100" y="100" width="200" height="120" rx="10" ry="10" fill="#2c3e50" stroke="#34495e" stroke-width="2"/>
  
  <!-- Servo Horn/Output Shaft -->
  <circle cx="200" cy="160" r="30" fill="#7f8c8d" stroke="#34495e" stroke-width="2"/>
  <circle cx="200" cy="160" r="25" fill="#95a5a6" stroke="#34495e" stroke-width="1"/>
  <circle cx="200" cy="160" r="5" fill="#34495e"/>
  
  <!-- Horn Splines -->
  <rect x="195" y="135" width="10" height="50" rx="2" ry="2" fill="#34495e"/>
  <rect x="175" y="155" width="50" height="10" rx="2" ry="2" fill="#34495e"/>
  
  <!-- Mounting Tabs -->
  <rect x="110" y="90" width="20" height="10" rx="2" ry="2" fill="#7f8c8d" stroke="#34495e" stroke-width="1"/>
  <rect x="270" y="90" width="20" height="10" rx="2" ry="2" fill="#7f8c8d" stroke="#34495e" stroke-width="1"/>
  <rect x="110" y="220" width="20" height="10" rx="2" ry="2" fill="#7f8c8d" stroke="#34495e" stroke-width="1"/>
  <rect x="270" y="220" width="20" height="10" rx="2" ry="2" fill="#7f8c8d" stroke="#34495e" stroke-width="1"/>
  
  <!-- Mounting Holes -->
  <circle cx="120" cy="95" r="3" fill="#34495e"/>
  <circle cx="280" cy="95" r="3" fill="#34495e"/>
  <circle cx="120" cy="225" r="3" fill="#34495e"/>
  <circle cx="280" cy="225" r="3" fill="#34495e"/>
  
  <!-- Wires -->
  <path d="M100 140 L70 140 L50 160" stroke="#e74c3c" stroke-width="3" fill="none"/>
  <path d="M100 160 L70 160 L50 180" stroke="#3498db" stroke-width="3" fill="none"/>
  <path d="M100 180 L70 180 L50 200" stroke="#f1c40f" stroke-width="3" fill="none"/>
  
  <!-- Labels -->
  <text x="40" y="160" font-family="Arial" font-size="12" fill="#e74c3c">Red (Power)</text>
  <text x="40" y="180" font-family="Arial" font-size="12" fill="#3498db">Black (Ground)</text>
  <text x="40" y="200" font-family="Arial" font-size="12" fill="#f1c40f">Yellow (Signal)</text>
  
  <text x="150" y="80" font-family="Arial" font-size="16" font-weight="bold" fill="#2c3e50">Standard Servo Motor</text>
</svg>