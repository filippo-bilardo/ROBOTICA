<svg xmlns="http://www.w3.org/2000/svg" width="500" height="300" viewBox="0 0 500 300">
  <!-- Arduino Board -->
  <rect x="50" y="50" width="200" height="150" rx="10" ry="10" fill="#00979D" />
  <rect x="70" y="70" width="160" height="110" rx="5" ry="5" fill="#E6E6E6" />
  <text x="150" y="125" font-family="Arial" font-size="14" text-anchor="middle" fill="#000">Arduino</text>
  
  <!-- Digital Pin -->
  <rect x="250" y="80" width="20" height="10" fill="#000" />
  <text x="260" y="75" font-family="Arial" font-size="10" text-anchor="middle" fill="#000">Pin 13</text>
  
  <!-- LED -->
  <circle cx="350" cy="100" r="15" fill="#FF0000" />
  <path d="M335 100 L365 100 M350 85 L350 115" stroke="#000" stroke-width="1" />
  
  <!-- Resistor -->
  <path d="M270 85 L300 85 L300 115 L330 115" stroke="#000" stroke-width="2" />
  <path d="M300 85 L300 115 L330 115 L330 85" stroke="#000" stroke-width="2" fill="none" />
  <text x="315" y="105" font-family="Arial" font-size="10" text-anchor="middle" fill="#000">220Î©</text>
  
  <!-- Wires -->
  <line x1="250" y1="85" x2="270" y2="85" stroke="#FF0000" stroke-width="2" />
  <line x1="330" y1="115" x2="350" y2="115" stroke="#000" stroke-width="2" />
  <line x1="350" y1="85" x2="350" y2="115" stroke="#000" stroke-width="2" />
  
  <!-- GND -->
  <rect x="250" y="150" width="20" height="10" fill="#000" />
  <text x="260" y="175" font-family="Arial" font-size="10" text-anchor="middle" fill="#000">GND</text>
  <line x1="260" y1="150" x2="260" y2="115" stroke="#000" stroke-width="2" />
  
  <!-- Labels -->
  <text x="350" y="75" font-family="Arial" font-size="12" text-anchor="middle" fill="#000">LED</text>
  <text x="350" y="140" font-family="Arial" font-size="10" text-anchor="middle" fill="#000">Catodo (-)</text>
  <text x="350" y="60" font-family="Arial" font-size="10" text-anchor="middle" fill="#000">Anodo (+)</text>
</svg>